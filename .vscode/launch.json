{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch server",
      "request": "launch",
      "runtimeArgs": ["run", "dev"],
      "runtimeExecutable": "npm",
      "skipFiles": ["node_modules/**"],
      "type": "node"
    },
    {
      "name": "Run Collector",
      "request": "launch",
      "runtimeArgs": [
        "run",
        "collector",
        "--",
        "--provider cinevood",
        "--link https://1cinevood.digital/"
      ],
      "runtimeExecutable": "npm",
      "skipFiles": ["node_modules/**"],
      "type": "node"
    }
  ]
}
